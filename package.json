{
  "name": "doz-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node development.js",
    "build": "parcel build public/index.html --no-cache --public-url ./",
    "test": "mocha --require ignore-styles --require babel-register \"./{,!(node_modules)/**}/*.test.js\"",
    "server": "node server/app.js",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm run-script build && git commit -a -m BUILD",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "doz": "^1.6.1",
    "doz-router": "^1.2.0",
    "doz-ssr": "^1.0.0",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "opn": "^5.3.0",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "postcss-modules": "^1.3.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
