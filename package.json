{
  "name": "doz-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node development.js",
    "build": "parcel build client/app.js --out-file bundle.js --no-cache --out-dir public/dist --public-url ./",
    "server": "node server/app.js",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm run-script build && git commit -a -m BUILD",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "doz": "^1.4.3",
    "doz-router": "^0.1.4",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "opn": "^5.3.0",
    "parcel-bundler": "^1.9.4",
    "postcss-modules": "^1.3.2"
  }
}
